x = [ 2, 3, 4, 5, 6, 8, 10, 20, 30 ]
print x[2:6]        # slice (sub-array)
print x[2:8:2]      # steps of 2

print x[:8]     # lower bound defaults to 0
print x[3:]     # upper bound defaults to the end
print x[:]      # default slice (creates a copy)
y = x[:]

a = [ [10, 20], [30, 40]]
b = a[:]
print id(b[0]), id(a[0])
