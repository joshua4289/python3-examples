class MyIterator:
    def __init__(self):
        self.x = 1
        self.y = 1
    
    def __iter__(self):
        return self
    
    def next(self):
        self.x, self.y = self.y, self.x + self.y
        return self.x

def fib():
    x = 1
    y = 1
    while x < 100:
        x, y = y, x + y
        yield x
    return
        
m = MyIterator()
x = m.__iter__()
print x.next()
print x.next()
print x.next()
print x.next()
print x.next()

x = fib()
print x.next()
print x.next()
print x.next()
print x.next()
print x.next()
