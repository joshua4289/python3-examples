class Time:
    def __init__(self, hrs, min):
        self.hrs = hrs
        self.min = min
    def display(self):
        print "{0} hrs, {1} min".format(self.hrs, self.min)
    def addOneHour(self): pass
    def addOneMinute(self): pass
t1 = Time(6, 40)
t2 = Time(3, 25)
t1.display()
t1.addOneHour()
t2.addOneMinute()