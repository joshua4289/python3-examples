class Point:
    def initialize(self, name, x0, y0):
        self.name = name
        self.x = x0
        self.y = y0

######### use the class ##########
# create objects

p1 = Point()
p2 = Point()
p3 = Point()

#     initialize(self, name0,   x0, y0):
p1.initialize("wendy", 12, 10)
p2.initialize("peter", 22, 20)
p3.initialize("tinkerbell",  32, 30)
print p1.name
print p1.__dict__['name']


