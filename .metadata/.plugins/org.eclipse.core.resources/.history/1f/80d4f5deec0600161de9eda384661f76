class Times:
    def __init__(self, hrs = 0, min = 0):
        self.hrs = hrs
        self.min = min
    def __add__(self, rhs):
        if isinstance(rhs, int):
            rhs = Times(rhs/60, rhs%60)
        result = Times()
        result.hrs = self.hrs + rhs.hrs
        result.min = self.min + rhs.min
        if result.min >= 60:
            result.min -= 60
            result.hrs += 1
        return result
    def __radd__(self, lhs):
        return lhs + self
    def __str__(self):
        return "[{:2d},{:02d}]".format(self.hrs, self.min)
            
t1 = Times(5, 45)
t2 = Times(3, 20)
t3 = t1 + t2
print t3
t4 = t1 + 130
print t4
t5 = 130 + t1
print t5


