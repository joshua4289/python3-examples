class Point:
    def initialize(self, name0, x0, y0):
        self.name = name0
        self.x = x0
        self.y = y0

######### use the class ##########
# create objects

p1 = Point()
p2 = Point()
p3 = Point()

#     initialize(self, name0,   x0, y0):
Point.initialize(p1  , "wendy", 12, 10)
Point.initialize(p2  , "peter", 22, 20)
Point.initialize(p3  , "tinkerbell",  32, 30)
print p1.__dict__
print p2.__dict__
print p3.__dict__

1

