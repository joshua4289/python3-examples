import sys, os, subprocess
from distutils.sysconfig import get_python_lib

import pip
print("pip.pyc is at {0}".format(pip.__path__))
print("Python exe: {0}".format(sys.executable))
print("Library: {0}".format(get_python_lib()))

subprocess.call("bash -c which pip-2.7".split())


print "DONE"
