# S = stockPrice = 210.59
# t = timeUntilOptionExpiresInDays = 4
# K = strikePrice = 205.0
# r = riskFreeInterestRate = 0.2175
# N = cumulativeStandatdNormalDistribution
# s = standardDeviationOfStockReturns
# d1 = ( log(S/K) + (r + s**2/2.0)*t ) / (s * t**0.5)
# d2 = d1 - s * t**0.5
# C = S * N(d1) - K * exp(-r*t) * N(d2)


from scipy.stats import norm
print norm.cdf(1.96)
