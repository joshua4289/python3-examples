def volume(height, width, length):
    h = height
    w = width
    l = length
    return h * (w + 4) * (l + 2)

print volume(10, 12, 14)
print volume(14, 10, 12)        # pass by position
print volume(height=14, width=10, length=12) # pass by name
print volume(length=12, height=14, width=10) # pass by name
