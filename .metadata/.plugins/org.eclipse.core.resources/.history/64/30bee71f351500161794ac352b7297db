import pandasx

class Point:
    # CTOR
    def __init__(self, x0, y0, name):
        self.x = x0 # self => object dictionary
        self.y = y0
        self.name = name
    def display(self):
        print self.x, self.y, self.name
    def moveBy(self, dx, dy):
        self.x = self.x + dx
        self.y = self.y + dy
# create 3 point objects
print Point.__dict__
p1 = Point(100, 200, 'darwin') # creates + initializes
p2 = Point(150, 170, 'tesla')
p3 = Point(250, 175, 'feynman')
p1.moveBy(1, 1)
p2.moveBy(1, 2)
p3.moveBy(1, 3)
p1.display()
p2.display()
p3.display()

