class Point:
    def initialize(self, name, x0, y0):
        self.name = name
        self.x = x0
        self.y = y0
        
# create objects
p1 = Point()
p2 = Point()
p3 = Point()
#     initialize(self, name,     x0,  y0):
print p1
Point.initialize(p1,  "first",  100, 200)
Point.initialize(p2,  "second", 300, 500)
Point.initialize(p3,  "third",  250, 100)
