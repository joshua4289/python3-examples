############################################################
#
#    decorators
#
############################################################

def f1(fn):
    def f2(x):
        def inner(y):
            return fn(x + y)
        return inner
    return f2

def doubleIt(n): return 2 * n
def trebleIt(n): return 3 * n
def quadrupleIt(n): return 4 * n


print f1(doubleIt)
1
