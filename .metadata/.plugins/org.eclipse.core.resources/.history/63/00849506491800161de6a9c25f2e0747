class Point:
    count = 0
    @staticmethod
    def getCount():
        return Point.count
    def initialize(self, name, x0, y0):
        Point.count += 1
        self.name = name
        self.x = x0
        self.y = y0
    def display(self):
        print "{:12s}[{},{}]".format(self.name, self.x, self.y)
    def moveBy(self, dx, dy):
        self.x += dx
        self.y += dy
print Point.getCount()
p1 = Point()
p2 = Point()
p3 = Point()
p1.initialize("wendy", 12, 10)
p2.initialize("peter", 22, 20)
p3.initialize("tinkerbell",  32, 30)
print Point.getCount()
p1.moveBy(100, 100)
p2.moveBy(100, 100)
p3.moveBy(100, 100)
p1.display()
p2.display()
p3.display()
