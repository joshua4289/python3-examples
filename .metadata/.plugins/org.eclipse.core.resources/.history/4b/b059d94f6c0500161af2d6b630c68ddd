salary = { 'John' : 50000, 
           'Susan': 47000,
           'Zoe'  : 60000,
           'Simon': 52000,
           'Ali'  : 36000,
           'Peter': 75000,
         }
theKeys = salary.keys()
theKeys.sort()
for key in theKeys:
    print key, salary[key]





