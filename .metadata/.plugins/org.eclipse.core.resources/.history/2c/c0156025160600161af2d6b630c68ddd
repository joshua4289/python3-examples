class Point:
    def initialize(self, name, x0, y0):
        self.name = name
        self.x = x0
        self.y = y0
    def display(self):
        print "Point: '{}' is at [{},{}]".format(self.name, self.x, self.y)
    def moveBy(self, dx, dy):
        self.x = self.x + dx
        self.y = self.y + dy
# create objects
p1 = Point()
p2 = Point()
p3 = Point()
p1.moveBy(1, 1)
p2.moveBy(1, 1)
p3.moveBy(1, 1)
p1.initialize("first",  100, 200)
p2.initialize("second", 300, 500)
p3.initialize("third",  250, 100)
p1.display()
p2.display()
p3.display()

1