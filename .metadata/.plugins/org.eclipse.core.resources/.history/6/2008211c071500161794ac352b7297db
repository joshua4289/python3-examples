salary = { 'bob' : 45000, 
           'sue' : 35000,
           'jimmy' : 50000,
           'zoe' : 47000,
           'tim' : 34000,
           'tom' : 42000
          }

topSalaries = [ 0, 0 ]
for key, value in salary.iteritems():
    if value > topSalaries[0]:
        topSalaries[0] = value

for key, value in salary.iteritems():
    if value > topSalaries[1] and value <= topSalaries[0]:
        topSalaries[1] = value

print topSalaries


    