# refactoring



def f(x, y):
    return lambda:x + y

def subtract_it(x, y):
    return x - y

def addit(x, y):
    result = x + y
class A:

    def __init__(self, x, y):
        self.x = x
        self.y = y

    self.x
    self.y


x = 100
y = 200

result = x + y


a = A(x, y)
addit(x, y)
addit(x, y)

result = subtract_it(x, y)
square = f(x, y)
z = f(3,4)
print z()

