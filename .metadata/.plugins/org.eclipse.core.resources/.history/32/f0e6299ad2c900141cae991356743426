############################################################
#
#    decorators
#
############################################################

def trace(fn):
    def enhance(x):
        print "calling " + fn.func_name + "(" + str(x) + ")"
        return fn(x)
    return enhance
    

def ff(fn):
    print "ff"
    return fn


class foo(object):
    @trace
    def xxx(self, f):
        print "foo"
x = foo()
x.xxx(5)
1
