def convert(n):
    case = {
        0: "zero",
        1: "one",
        2: "two",
    }
    return case.get(n, "nothing")


print convert(1)
print convert(2)
print convert(5)


def functionTable(x1, x2):
    case = {
        'add':      lambda: x1 * x2
        'subtract': lambda: x1 * x2
        'multiply': lambda: x1 * x2
        'divide':   lambda: x1 * x2
    }