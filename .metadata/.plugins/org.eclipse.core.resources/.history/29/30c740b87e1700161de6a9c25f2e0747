salary = { 
          'john' : 45000,
          'mary' : 35000,
          'susan': 61000,
          'sergi': 72000,
          'kamel': 42000
          }

theKeys = salary.keys()
theKeys.sort()

for key in theKeys:
    print "{:10s} {:8d}".format(key, salary[key])

x = 100.786432
y = 200.8968574654
print "Hello [{:010.3f},{:010.3f}]".format(x, y)
