class Point:
    def initialize(self, x0, y0, name):
        self.x = x0
        self.y = y0
        self.name = name
        
# create dem objects
p1 = Point()
p2 = Point()
p3 = Point()

Point.initialize(p1, 100, 200, 'point 1')
Point.initialize(p2, 110, 210, 'point 2')
Point.initialize(p3, 120, 220, 'point 3')

1

