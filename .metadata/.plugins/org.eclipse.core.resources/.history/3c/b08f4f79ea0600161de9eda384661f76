class Time:
    def __init__(self, hrs = 0, min = 0):
        self.hrs = hrs
        self.min = min
    def __add__(self, rhs):
        result = Time()
        result.hrs = self.hrs + rhs.hrs
        result.min = self.min + rhs.min
        if result.min >= 60:
            result.min -= 60
            result.hrs += 1
        return result
    def __str__(self):
        "[{},{}]".format(self.hrs, self.min)
            
t1 = Time(5, 45)
t2 = Time(3, 20)
t3 = t1 + t2
print t3

