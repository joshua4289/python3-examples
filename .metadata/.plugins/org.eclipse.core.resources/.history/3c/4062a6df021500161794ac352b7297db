salary = { 'bob' : 45000, 
           'sue' : 55000,
           'jimmy' : 50000,
           'zoe' : 47000,
           'tim' : 34000
          }

print salary
print "{:10s}{:>10s}".format("key", "value")
print "{:10s}{:>10s}".format("===", "=====")
for key, value in salary.iteritems():
    print "{:10s}{:10d}".format(key, value)

print "XXXXJimmmy's salary =", salary['jimmy']
