class Point:
    def moveBy(self, dx, dy):
        self.x = self.x + dx
        self.y += dy
    # CTOR
    def __init__(self, x0, y0, name):
        self.x = x0
        self.y = y0
        self.name = name
    def __init__(self, name):
        self.x = 0
        self.y = 0
        self.name = name
        
        
    def display(self):
        print "Point: {0} [{1}, {2}]".format(self.name, self.x, self.y)
# define objects
q = Point("origin")
p1 = Point(100, 300, "bob")
p2 = Point(200, 500, "zoe")
p3 = Point(400, 200, "jim")
p1.moveBy(1, 1)
p2.moveBy(1, 1)
p3.moveBy(1, 1)
p1.display()
p2.display()
p3.display()

1
