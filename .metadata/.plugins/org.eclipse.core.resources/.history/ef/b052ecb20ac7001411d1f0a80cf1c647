class Point:
    def initialize(self, x0, y0, name):
        self.x = x0
        self.y = y0
        self.name = name
    def moveBy(self, dx, dy):
        self.x = self.x + dx
        self.y = self.y + dy
    def display(self):
        print "Point: {2} is at {0},{1}".format(self.x, self.y, self.name)
# create objects
p1 = Point()
p2 = Point()
p3 = Point()
p1.initialize(100, 200, "Point-p1")
p2.initialize(110, 300, "Point-p2")
p3.initialize(150, 400, "Point-p3")
p1.moveBy(1, 1)
p2.moveBy(1, 1)
p3.moveBy(1, 1)
p1.display()
p2.display()
p3.display()
1
