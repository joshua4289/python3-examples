import random, time

count = 0
n = 10
def f1():
    global count
    for i in xrange(n):
        time.sleep(random.random() * 0.001)
        count = count + i

def f2():
    global count
    for i in xrange(2 * n):
        time.sleep(random.random() * 0.001)
        count = count + i

def f3():
    global count
    for i in xrange(3 * n):
        time.sleep(random.random() * 0.001)
        count = count + i

def f4():
    global count
    for i in xrange(4 * n):
        time.sleep(random.random() * 0.001)
        count = count + i


def foo():
    f1()
    f2()
    f3()
    f4()
    
